From: Joachim Breitner <mail@joachim-breitner.de>
Date: Mon, 9 Nov 2009 11:47:44 +0100
Subject: Change make to gmake

---
 3rd/defaults.pro            |    2 +-
 3rd/qt/qt-unix.pro          |    2 +-
 defaults.pro                |    2 +-
 serna/app/get_undef_syms.sh |    2 +-
 4 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/3rd/defaults.pro b/3rd/defaults.pro
index 70fc820..b82c42d 100644
--- a/3rd/defaults.pro
+++ b/3rd/defaults.pro
@@ -15,7 +15,7 @@ unix:TMAKE_RANLIB   = ranlib
 unix:TMAKE_LIBS     *= -L/usr/local/lib
 
 unix:PACKAGE_CONFIGURE  = ./configure
-unix:MAKE               = gmake
+unix:MAKE               = make
 
 static:UNIX_LIB_EXT     = .a
 dynamic:UNIX_LIB_EXT    = .so
diff --git a/3rd/qt/qt-unix.pro b/3rd/qt/qt-unix.pro
index 2c92e65..9dc01cc 100644
--- a/3rd/qt/qt-unix.pro
+++ b/3rd/qt/qt-unix.pro
@@ -2,7 +2,7 @@
 #
 
 PACKAGE_CONFIGURE   = ./configure
-PACKAGE_MAKE        = gmake
+PACKAGE_MAKE        = make
 PACKAGE_MAKEFILE    = Makefile
 PACKAGE_MAKEFILEOPT = -f
 PACKAGE_CLEANOPTS   = clean
diff --git a/defaults.pro b/defaults.pro
index 6e8fcc7..6297df9 100644
--- a/defaults.pro
+++ b/defaults.pro
@@ -56,7 +56,7 @@ linux:DEFINES *= __STDC_VERSION__=199901L
 unix:TMAKE_RANLIB   = ranlib
 unix:TMAKE_LIBS     *= -L/usr/local/lib
 darwin:TMAKE_LIBS   -= -L/usr/local/lib
-unix:MAKE            = gmake
+unix:MAKE            = make
 unix:COPY            = cp
 unix:BISON           = bison
 unix:TMAKE_CXXFLAGS  += -g
diff --git a/serna/app/get_undef_syms.sh b/serna/app/get_undef_syms.sh
index b1d4ee0..0c8740d 100755
--- a/serna/app/get_undef_syms.sh
+++ b/serna/app/get_undef_syms.sh
@@ -80,7 +80,7 @@ save_libs $plugin
 
 cd ${plugin_dir}
 COLLECT_NO_DEMANGLE=Y; export COLLECT_NO_DEMANGLE
-gmake LD_SHLIB_UNDEF="${LD_NOUNDEF}" ${TARGET} 2>&1 | cat > ${OUTFILE}
+make LD_SHLIB_UNDEF="${LD_NOUNDEF}" ${TARGET} 2>&1 | cat > ${OUTFILE}
 RC=1
 if ${GREP} -q "${UNDEF_ERRMSG}" ${OUTFILE}; then 
     RC=0
